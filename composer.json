{
  "name": "nyra/dhl",
  "description": "Deutsche Post INTERNETMARKE (Post & Paket Deutschland)",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Nyra\\Dhl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nyra\\Internetmarke\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "analyse": "phpstan analyse src --level=max",
    "cs": "php-cs-fixer fix --config=.php_cs.dist --allow-risky=yes"
  },
  "authors": [
    {
      "name": "René Preuß",
      "email": "rene@preuss.io"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.8",
    "guzzlehttp/psr7": "^2.6",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0 || ^2.0",
    "symfony/serializer": "^7.0",
    "symfony/property-access": "^7.0",
    "symfony/property-info": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.11",
    "friendsofphp/php-cs-fixer": "^3.36"
  }
}
